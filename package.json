{
  "name": "@foxxmd/komodo-import",
  "version": "0.2.2",
  "description": "",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "install:parallel": "concurrently --kill-others-on-fail --names app,docs \"npm ci --no-audit\" \"npm run docs:install\"",
    "test": "mocha --reporter spec",
    "build:backend": "tsc -p src",
    "dev": "nodemon -w src -x tsx src/index.ts",
    "start": "NODE_ENV=production tsx src/index.ts",
    "build:parallel": "concurrently --kill-others-on-fail --names backend,frontend,docs \"npm run -s build:backend\" \"npm run -s build:frontend\" \"npm run docs:build\"",
    "docs:install": "cd docsite && npm ci --no-audit",
    "docs:start": "cd docsite && npm start",
    "docs:build": "cd docsite && npm run build",
    "demo": "HOST_PARENT_PATH=/my/cool/path SERVER_NAME=my-cool-server FILES_ON_SERVER_DIR=tests/exampleDir tsx src/index.ts"
  },
  "engines": {
    "node": ">=18.19.1",
    "npm": ">=10.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FoxxMD/komodo-import.git"
  },
